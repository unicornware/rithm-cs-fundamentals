    "sinon": "13.0.1",
    "sinon-chai": "3.7.0",
    "trash-cli": "4.0.0",
    "ts-mocha": "9.0.2",
    "ts-node": "10.7.0",
    "tsconfig": "7.0.0",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.6.2"
  },
  "resolutions": {
    "dotenv": "16.0.0"
  },
  "engines": {
    "node": ">=14.19.1",
    "npm": "use-yarn",
    "yarn": ">=3.2.0"
  },
  "packageManager": "yarn@3.2.0"
