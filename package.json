    "@types/sinon-chai": "3.2.8",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "chai": "4.3.4",
    "concurrently": "7.0.0",
    "dotenv-cli": "5.0.0",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-import-resolver-typescript": "2.7.0",
